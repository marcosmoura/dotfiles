[
  {
    "bindings": {
      "cmd-shift-e": "workspace::ToggleRightDock",
      "cmd-shift-b": "workspace::ToggleBottomDock"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "cmd-k l": "editor::ConvertToLowerCase",
      "cmd-k u": "editor::ConvertToUpperCase",
      "cmd-k c": "editor::ConvertToLowerCamelCase",
      "cmd-k m": "language_selector::Toggle",
      "cmd-y": "editor::Redo",
      "cmd-{": "pane::SplitLeft",
      "cmd-}": "pane::SplitUp"
    }
  }
]
