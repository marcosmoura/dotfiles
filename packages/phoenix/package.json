{
  "name": "phoenix-config",
  "version": "0.1.0",
  "private": true,
  "description": "Phoenix Config",
  "bugs": {
    "url": "https://github.com/marcosmoura/dotfiles/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcosmoura/dotfiles"
  },
  "license": "MIT",
  "author": {
    "name": "marcosmoura",
    "email": "marcosvmmoura@gmail.com"
  },
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "tsx scripts/build.ts",
    "clean-install": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "lint": "pnpm run lint:prettier && pnpm run lint:eslint && pnpm run lint:ts-check",
    "lint:eslint": "eslint ./src --ext .ts --format=pretty --fix --cache --cache-location ./node_modules/.eslintcache",
    "lint:prettier": "prettier --write --list-different '**/*'",
    "lint:ts-check": "tsc --noEmit",
    "start": "tsx scripts/build.ts watch"
  },
  "dependencies": {
    "just-extend": "^6.2.0",
    "just-split": "^3.2.0",
    "p-queue": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "@types/phoenix": "github:mafredri/phoenix-typings",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.7.0",
    "chalk": "^5.3.0",
    "esbuild": "^0.23.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tsdoc": "^0.3.0",
    "eslint-plugin-unicorn": "55.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.5.1",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">= 20.0"
  }
}
