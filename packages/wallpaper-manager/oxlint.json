{
  "env": {
    "node": true
  },
  "rules": {
    "no-unused-vars": [
      2,
      {
        "args": "none",
        "caughtErrors": "none",
        "varsIgnorePattern": "^h|React|createElement|Fragment$"
      }
    ],
    "typescript/no-namespace": 0,
    "no-constant-binary-expression": 0,
    "no-useless-catch": 0,
    "no-empty-pattern": 0,
    "prefer-rest-params": 0,
    "prefer-spread": 0,
    "no-cond-assign": 0,
    "unicorn/no-new-array": 0,
    "unicorn/prefer-string-starts-ends-with": 0
  }
}
